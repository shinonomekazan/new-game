{
	"name": "akashic-template",
	"version": "0.1.1",
	"description": "",
	"scripts": {
		"clean": "rimraf ./script",
		"watch": "webpack -w",
		"build": "webpack",
		"build:production": "NODE_ENV=production webpack",
		"update": "akashic-cli-scan asset && akashic-cli-scan globalScripts",
		"preexport:content": "npm run build:production",
		"prettier:check": "prettier --check .",
		"prettier:write": "prettier --write .",
		"lint": "npm run prettier:check",
		"lint:fix": "npm run prettier:write",
		"start": "akashic-sandbox .",
		"serve": "akashic-cli-serve --no-open-browser",
		"export:zip": "rimraf game.zip && akashic-cli-export-zip -o game.zip -s",
		"export:html": "rimraf game && akashic-cli-export-html -o game",
		"export:content": "rimraf game.zip && akashic-cli-export-zip -o ./game.zip --nicolive",
		"test": "npm run lint && jest",
		"t": "akashic-cli-serve .",
		"generate": "ts-node editor/generate.ts"
	},
	"author": "Shinonomekazan Co., Ltd.",
	"license": "MIT",
	"devDependencies": {
		"@actions/core": "^1.10.1",
		"@akashic/akashic-cli-export": "^1.8.17",
		"@akashic/akashic-cli-scan": "^0.16.16",
		"@akashic/akashic-cli-serve": "^1.15.26",
		"@akashic/akashic-engine": "^3.12.0",
		"@akashic/akashic-sandbox": "^0.23.0",
		"@akashic/headless-akashic": "^4.0.0",
		"@types/fs-extra": "^11.0.4",
		"@types/jest": "^29.1.1",
		"@types/node": "^18.7.23",
		"jest": "^29.1.2",
		"prettier": "^2.7.1",
		"rimraf": "^3.0.2",
		"ts-jest": "^29.0.3",
		"ts-loader": "^9.4.1",
		"ts-node": "^10.9.2",
		"typescript": "^4.8.4",
		"webpack": "^5.74.0",
		"webpack-cli": "^4.10.0"
	},
	"dependencies": {
		"@akashic/instance-storage-plugin": "file:lib/instance-storage-plugin-0.3.0",
		"@akashic-extension/akashic-label": "^3.1.0",
		"@akashic-extension/akashic-timeline": "^3.2.1",
		"@akashic-extension/collision-js": "^1.1.0",
		"@akashic-extension/raycaster-js": "^1.0.1"
	}
}
